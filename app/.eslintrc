{
  "env": {
    "browser": true,
    "builtin": true,
    "jquery": true,
    "node": false
  },
  "globals": {
    "router": false
  },
  "rules": {
    "no-bitwise": 2,
    "no-caller": 2,
    "no-console": 0,
    "no-extend-native": 2,
    "no-multi-str": 2,
    "no-proto": 2,
    "strict": [2, "global"],
    "valid-typeof": 2
  }
}
