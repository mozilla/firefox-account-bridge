{
  "env": {
    "amd": true,
    "es6": true,
    "mocha": true,
    "node": true
  },
  "rules": {
    "camelcase": 0,
    "comma-dangle": 0,
    "complexity": [2, 6],
    "consistent-return": 0,
    "curly": [2, "all"],
    "dot-notation": 0,
    "eol-last": 2,
    "eqeqeq": [2, "allow-null"],
    "global-strict": 0,
    "handle-callback-err": 0,
    "indent": [2, 2],
    "key-spacing": 0,
    "new-cap": 0,
    "no-cond-assign": [2, "except-parens"],
    "no-debugger": 2,
    "no-empty": 0,
    "no-eval": 2,
    "no-irregular-whitespace": 2,
    "no-loop-func": 0,
    "no-multiple-empty-lines": [2, {"max": 2}],
    "no-multi-spaces": 0,
    "no-new": 2,
    "no-script-url": 2,
    "no-sequences": 2,
    "no-shadow": 0,
    "no-spaced-func": 0,
    "no-trailing-spaces": 2,
    "no-undef": 2,
    "no-underscore-dangle": 0,
    "no-unused-vars": [2, {"vars": "all", "args": "none"}],
    "no-use-before-define": [2, "nofunc"],
    "no-with": 2,
    "quotes": [2, "single", "avoid-escape"],
    "semi": [2, "always"],
    "space-unary-ops": 0,
    "strict": [2, "global"],
    "valid-typeof": 1,
    "wrap-iife": 0,
    "yoda": 0
  }
}
